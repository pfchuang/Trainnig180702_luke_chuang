<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

  <object id="BookController"  type="BMS.Controllers.BookController,BMS" singleton="false">
    <property name="codeService" ref="CodeService" />
    <property name="bookService" ref="BookService" />
  </object>

  <object id="CodeService" type="BMS.Service.CodeService,BMS.Service">
    <property name="codeDao" ref="CodeDao" />
  </object>

  <object id="BookService" type="BMS.Service.BookService,BMS.Service">
    <property name="bookDao" ref="BookDao" />
  </object>

  <object id="CodeDao" type="BMS.Dao.CodeDao,BMS.Dao"/>
  <object id="BookDao" type="BMS.Dao.BookDao,BMS.Dao"/>

</objects>